{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "Front end task from Max",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup/popup.html"
    }
  }